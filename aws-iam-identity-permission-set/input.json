{
	"awsCredentials": {
		"account_id": "<aws_account_id>",
		"account_name": "<aws_account_name>",
		"role_name": "AdministratorAccess",
		"region_name": "<aws_region>",
		"profile_name": "<aws_profile_name>",
		"role_arn": "<aws_role_arn>",
		"access_key": "<aws_access_key>",
		"secret_access_key": "<aws_secret_key>",
		"key_session_token": "<aws_session_token>"
	},
	"smtpCredentials": {
		"host": "<smtp_host>",
		"port": "<smtp_port>",
		"username": "<smtp_username>",
		"password": "<smtp_password>",
		"from_email": "<sender_email>"
	},
	"emailNotification": {
		"email_subject": "AWS IAM Identity Center Permission Set Automation Report",
		"subject_prefix": "Example",
		"to": [
			"<recipient_email>"
		],
		"cc": [
			"<cc_email>"
		],
		"bcc": [
			"<bcc_email>"
		]
	},
	"defaultTags": {
		"Environment": "ALL",
		"Project": "Automation",
		"Owner": "DevOps Team",
		"CreatedBy": "AutomationScript",
		"ManagedBy": "IAMIdentityCenterPermissionSetAutomation"
	},
	"defaultIAMPolicy": {
		"deny-admin-policy": {
			"policy_name": "iam-identity-center-deny-admin-policy",
			"description": "Deny administrative and sensitive actions",
			"policy_file": "deny-admin-policy.json"
		},
		"deny-developer-policy": {
			"policy_name": "iam-identity-center-deny-developer-policy",
			"description": "Deny developer sensitive actions",
			"policy_file": "deny-developer-policy.json"
		},
		"allow-admin-policy": {
			"policy_name": "iam-identity-center-allow-admin-policy",
			"description": "Allow administrative and sensitive actions",
			"policy_file": "allow-admin-policy.json"
		},
		"allow-developer-policy": {
			"policy_name": "iam-identity-center-allow-developer-policy",
			"description": "Allow developer sensitive actions",
			"policy_file": "allow-developer-policy.json"
		}
	},
	"permissionSetConfig": {
		"AWS-UAT-ADMIN": {
			"permission_set_name": "AWS-UAT-ADMIN",
			"description": "Administrator Access for XCube AWS UAT Account",
			"session_duration": "PT8H",
			"aws_managed_policies": [
				"ReadOnlyAccess"
			],
			"customer_managed_policies": [
				"iam-identity-center-deny-admin-policy",
				"iam-identity-center-allow-admin-policy"
			],
			"inline_policy": ""
		},
		"AWS-UAT-DEVELOPER": {
			"permission_set_name": "AWS-UAT-DEVELOPER",
			"description": "Developer Access for XCube AWS UAT Account",
			"session_duration": "PT8H",
			"aws_managed_policies": [
				"ReadOnlyAccess"
			],
			"customer_managed_policies": [
				"iam-identity-center-deny-developer-policy",
				"iam-identity-center-allow-developer-policy"
			],
			"inline_policy": ""
		},
		"AWS-PROD-ADMIN": {
			"permission_set_name": "AWS-PROD-ADMIN",
			"description": "Administrator Access for XCube AWS PROD Account",
			"session_duration": "PT8H",
			"aws_managed_policies": [
				"ReadOnlyAccess"
			],
			"customer_managed_policies": [
				"iam-identity-center-deny-admin-policy",
				"iam-identity-center-allow-admin-policy"
			],
			"inline_policy": ""
		},
		"AWS-PROD-DEVELOPER": {
			"permission_set_name": "AWS-PROD-DEVELOPER",
			"description": "Developer Access for XCube AWS PROD Account",
			"session_duration": "PT8H",
			"aws_managed_policies": [
				"ReadOnlyAccess"
			],
			"customer_managed_policies": [
				"iam-identity-center-deny-developer-policy",
				"iam-identity-center-allow-developer-policy"
			],
			"inline_policy": ""
		}
	}
